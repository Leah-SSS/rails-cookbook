<h1><%= "Add Recipe to #{@category.name}" %></h1>

<%= simple_form_for [@category, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :recipe %>
    <%= f.submit class: "btn btn-primary"%>
<% end %>

<div class="btn btn-info"><%= link_to "Return to Category", category_path(@category)%></div>
